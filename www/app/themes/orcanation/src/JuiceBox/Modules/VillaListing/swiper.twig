<div class="swiper {{class}}">
	<div class="swiper-wrapper">
		{% for item in arr %}
			<div class="swiper-slide">
				<div class="swiper-slide-image">
					{% include 'partials/image.twig' with{
                    'image': item.url,
                    } %}
				</div>
			</div>
		{% endfor %}
	</div>
	<div class="swiper-navigation">
		<div class="swiper-slider-navigation-prev">
			{% include 'img/villa-listing-chevron.twig' with{
				class: 'icon left'
			} %}
		</div>
		<div class="swiper-slider-navigation-next">
			{% include 'img/villa-listing-chevron.twig' with{
				class: 'icon right'
			} %}
		</div>
	</div>
</div>
