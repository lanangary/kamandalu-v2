<div class="header" id="main-header">
	<div class="container container-header">
		<div class="header-wrap">
			<div class="header-section">
				<div class="header-section-wrap">
					<div class="header-section-wrap-toogle">
						{% include 'img/hamburger.svg' %}
					</div>
				</div>
			</div>

			<div class="header-section">
				<div class="header-section-wrap">
					<div class="header-section-wrap-logo">
						{% include 'img/logo.twig' %}
					</div>
					<div class="header-section-wrap-menu eaves t-14 color-white lh-120 t-uppercase">
						{% include 'Header/simple-nav.twig' with {
                            menu: theme_menus.header_menu
                        } only %}
					</div>
				</div>
			</div>

			<div class="header-section">
				<div class="header-section-wrap">

					{% for item in options.header_button %}
						{% set style = 'mbtn-transparent' %}

						{% if loop.last %}
							{% set style = 'mbtn-white' %}
						{% endif %}


						<div class="header-section-wrap-button ">
							{% include 'partials/button.twig' with {
							style: 'mbtn mbtn-mini' ~ ' ' ~ style,
							link: item.link
						} %}
						</div>
					{% endfor %}

				</div>
			</div>
		</div>
	</div>
</div>
